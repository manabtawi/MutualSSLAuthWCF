<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>

  <appSettings>
    <!-- Set Service endpoint URL -->
    <add key="endpointURL" value="https://www.example.com/service.svc"/>

    <!-- Set Service Certificate Serial Number, a trusted SSL certificate -->
    <add key="serviceCertificate" value="XXXXXXXXXX"/>

    <!-- Set Client Certificate Serial Number -->
    <add key="serviceCertificate" value="XXXXXXXXXX"/>

  </appSettings>
</configuration>